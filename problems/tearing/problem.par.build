 $BASE_DOMAIN
    n_d = 128, 128, 5
    bnd_xl = 'per'
    bnd_xr = 'per'
    bnd_yl = 'per'
    bnd_yr = 'per'
    bnd_zl = 'per'
    bnd_zr = 'per'
    xmin   =-0.5
    xmax   = 0.5
    ymin   =-0.5
    ymax   = 0.5
    zmin   =-0.5
    zmax   = 0.5
 /

 $MPI_BLOCKS
 /

 $UNITS
 /

 $RESTART_CONTROL
    restart  = 'last'
    new_id   = 'new'
    nrestart = 0
 /

 $END_CONTROL
    tend   = 10.0
    nend   = 10
 /

 $OUTPUT_CONTROL
    problem_name = 'tearing'
    run_id       = 'tst'
    dt_hdf  = 0.01
    dt_log  = 0.001
    dt_tsl  = 0.1
    dt_res  = 3.0
    vars(1:) = 'ener', 'dens', 'magx', 'magy', 'magz', 'velx', 'vely', 'velz'
 /

 $FLUID_IONIZED
    gamma  = 1.666666666
 /

  $NUMERICAL_SETUP
    cfl    = 0.4
    smalld = 1.e-5
    smallei= 1.e-5
    integration_order  = 2
    limiter= 'vanleer'
 /

 $GRAVITY
 /

 $RESISTIVITY
    eta_scale   =  4
    cfl_resist  =  0.4
    eta_0       =  1.e-6
    eta_1       =  1.0e-4
    j_crit      =  100.0
    deint_max   =  0.001
 /

 $PROBLEM_CONTROL
    beta         =  1.0
    v0           =  0.1
 /

 $INTERACTIONS
 /

 $AMR
 /
