 $BASE_DOMAIN
    n_d = 256, 256, 1024
    bnd_xl = 'per'
    bnd_xr = 'per'
    bnd_yl = 'per'
    bnd_yr = 'per'
    bnd_zl = 'per'
    bnd_zr = 'per'
    xmin   = -1000.0
    xmax   =  1000.0
    ymin   = -1000.0
    ymax   =  1000.0
    zmin   = -1000.0
    zmax   =  7000.0
 /

 $MPI_BLOCKS
 /

 $UNITS
    units_set = "user"
    cm   = 3.2407792894189803e-22   !   length_unit = 1 kpc
    gram = 2.1619676645392735e-39   !   mass_unit   = 2.3262e5 M_sun
    sek  = 3.241028514145478e-17    !   time_unit   = 0.9777 Gyr
 /

 $RESTART_CONTROL
    restart  = 'last'
    res_id   = ''
    nrestart = 0
 /

 $END_CONTROL
    tend   = 6.0
    nend   = 100000
 /

 $OUTPUT_CONTROL
    problem_name ='blob'
    run_id  =  'tst'
    dt_hdf  = 0.5666666666667e0
    dt_res  = 100.0e0
    dt_log  = 0.001
    dt_tsl  = 0.00001
    vars(1:) = 'dens', 'velx', 'vely', "velz", 'ener', "pres", "ethr"
 /

 $FLUID_NEUTRAL
    gamma  = 1.666666667
 /

 $NUMERICAL_SETUP
    cfl    = 0.9
    smalld = 1.e-10
    smallei= 1.e-10
    limiter= 'vanleer'
 /

 $PROBLEM_CONTROL
    ICfile = "wengen3.hi.h5"
! http://www.astrosim.net/code/lib/exe/fetch.php?media=wengen3.hi.h5
 /

 $INTERACTIONS
 /

 $AMR
 /
